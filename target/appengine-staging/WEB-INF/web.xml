<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>HelloAppEngine</servlet-name>
		<servlet-class>com.ase2017.ats.HelloAppEngine</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CreateTutorialServlet</servlet-name>
		<servlet-class>com.ase2017.ats.CreateTutorialServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>JoinTutorialServlet</servlet-name>
		<servlet-class>com.ase2017.ats.JoinTutorialServlet</servlet-class>
	</servlet>
	
	<servlet>
      <servlet-name>RestletServlet</servlet-name>
      <servlet-class>org.restlet.ext.servlet.ServerServlet</servlet-class>
      <init-param>
              <param-name>org.restlet.application</param-name>
              <param-value>com.ase2017.ats.StudentApplication</param-value>
      </init-param>
     </servlet>
     
     <servlet>
      <servlet-name>RestletServlet2</servlet-name>
      <servlet-class>org.restlet.ext.servlet.ServerServlet</servlet-class>
      <init-param>
              <param-name>org.restlet.application</param-name>
              <param-value>com.ase2017.ats.AttendanceLogApplication</param-value>
      </init-param>
     </servlet>
     
     <servlet>
      <servlet-name>RestletServletHelloWorld</servlet-name>
      <servlet-class>org.restlet.ext.servlet.ServerServlet</servlet-class>
      <init-param>
              <param-name>org.restlet.application</param-name>
              <param-value>com.ase2017.ats.FirstStepsApplication</param-value>
      </init-param>
     </servlet>

