	<!-- JSPC servlet mappings start -->

    <servlet>
        <servlet-name>org.apache.jsp.Attendance_005fList_jsp</servlet-name>
        <servlet-class>org.apache.jsp.Attendance_005fList_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.Attendance_005fList_005fall_jsp</servlet-name>
        <servlet-class>org.apache.jsp.Attendance_005fList_005fall_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.Attendance_005fLog_jsp</servlet-name>
        <servlet-class>org.apache.jsp.Attendance_005fLog_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.Main_005fPage_jsp</servlet-name>
        <servlet-class>org.apache.jsp.Main_005fPage_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.User_005fInfo_jsp</servlet-name>
        <servlet-class>org.apache.jsp.User_005fInfo_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.WEB_002dINF.Main_005fPage_jsp</servlet-name>
        <servlet-class>org.apache.jsp.WEB_002dINF.Main_005fPage_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.Attendance_005fList_jsp</servlet-name>
        <url-pattern>/Attendance_List.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.Attendance_005fList_005fall_jsp</servlet-name>
        <url-pattern>/Attendance_List_all.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.Attendance_005fLog_jsp</servlet-name>
        <url-pattern>/Attendance_Log.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.Main_005fPage_jsp</servlet-name>
        <url-pattern>/Main_Page.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.User_005fInfo_jsp</servlet-name>
        <url-pattern>/User_Info.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.WEB_002dINF.Main_005fPage_jsp</servlet-name>
        <url-pattern>/WEB-INF/Main_Page.jsp</url-pattern>
    </servlet-mapping>

<!-- JSPC servlet mappings end -->

<servlet-mapping>
		<servlet-name>HelloAppEngine</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CreateTutorialServlet</servlet-name>
		<url-pattern>/CreateTutorialServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>JoinTutorialServlet</servlet-name>
		<url-pattern>/JoinTutorialServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
      <servlet-name>RestletServlet</servlet-name>
      <url-pattern>/StudentXML/*</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
      <servlet-name>RestletServlet2</servlet-name>
      <url-pattern>/AttendanceLog/*</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
      <servlet-name>RestletServletHelloWorld</servlet-name>
      <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
	
